  "description": "Formula Edit, jison, calculator, editor",
  "main": "src/module.js",
  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "dev": "webpack --config webpack.config.dev.js",
    "start": "webpack-dev-server --progress --colors --mode development --config webpack.hot.js",
    "watch": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --config jest.config.js",
    "lexer": "jison ./src/lexer/cal.jison -o ./src/lexer/cal.js"
  },
